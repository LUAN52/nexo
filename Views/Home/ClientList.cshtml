@model List<Client>

<h1 class="subTtitle">Lista de clientes </h1>
<table class="table">
  <thead>
    <tr>
      
      <th scope="col">nome</th>
      <th scope="col">status</th>
    </tr>
  </thead>
  <tbody>
   
        @foreach (var item in Model)
        {    
            <tr>
                <td>@item.UserName</td>
                <td>@item.Status</td>
            </tr>  
        }
    
  </tbody>
</table>